<div class="container">
  <div class="row">
    <div class="col">
      <h3>
        Inicio de sesion
      </h3>
    </div>
  </div>


<div class="row">
  <div class="col col-xl-6 col-lg-6 col-md-6 col-sm-12">
    <%= form_for(resource, as: resource_name ,url: session_path(resource_name)) do |f| %>
      <div class="form-group">
        <%= f.label :email, "Correo electronico" %><br />
        <%= f.email_field :email, autofocus: true, class: "form-control" ,autocomplete: "email" %>
      </div>

      <div class="form-group">
        <%= f.label :password , "ContraseÃ±a"%><br />
        <%= f.password_field :password, class: "form-control" ,autocomplete: "current-password" %>
      </div>

      <div class="form-group">
        <%= f.submit "Iniciar sesion", class: "btn btn-primary btn-block" %>

      </div>
    <% end %>
  </div>
</div>
